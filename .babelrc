{
  "presets": ["es2015", "stage-0"],
  "plugins": [
    ["transform-class-properties", { "spec": true }],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "utils": "./server/utils",
          "config": "./server/configs",
          "src": "./src",
          "~": "./"
        }
      }
    ]
  ]
}
